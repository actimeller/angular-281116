{
  "compilerOptions": {
    "target": "es5",
    "lib":["es5","dom","es2015.promise","es2015.iterable"],
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "typeRoots": ["./node_modules/@types"],
    "types": ["whatwg-fetch"]
  },
  "exclude": [
    "node_modules"
  ]
}